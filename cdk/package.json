{
  "name": "blandmockapi-cdk",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Bland Mock API",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy:lambda": "cdk deploy BlandMockApiLambdaStack",
    "deploy:ecs": "cdk deploy BlandMockApiEcsStack",
    "deploy:apprunner": "cdk deploy BlandMockApiAppRunnerStack",
    "deploy:all": "cdk deploy --all",
    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "aws-cdk": "^2.140.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.140.0",
    "constructs": "^10.3.0"
  }
}
